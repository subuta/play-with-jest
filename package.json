{
  "name": "play-with-jest",
  "version": "1.0.0",
  "description": "Jest testing example",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=./src node -r esm ./src/server.js",
    "test": "jest --runInBand",
    "migrate": "knex-migrate up",
    "migrate:down": "knex-migrate down",
    "migrate:drop": "knex-migrate down --to 0",
    "migrate:reset": "run-s migrate:drop migrate seed",
    "seed": "node -r esm ./node_modules/.bin/knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-jest.git"
  },
  "keywords": [],
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-jest/issues"
  },
  "homepage": "https://github.com/subuta/play-with-jest#readme",
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "mocha",
      "jest",
      "jasmine"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/?(*.)+(spec).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "babel-jest": "^24.8.0",
    "core-js": "^2.6.9",
    "consola": "^2.9.0",
    "d3-format": "^1.3.2",
    "esm": "^3.2.25",
    "knex": "^0.19.5",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compose": "^4.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "jest": "^24.8.0",
    "knex-migrate": "^1.7.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^7.3.2",
    "standard": "^12.0.1",
    "supertest": "^4.0.2",
    "tempy": "^0.3.0"
  }
}
